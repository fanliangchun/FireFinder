

<%= simple_form_for [:admin, @app] do |f| %>
<%= f.input :title %>
<%= f.input :description %>
<%= f.input :images, as: :file %>
<%= select_tag(:category_id, options_for_select(@categories), :prompt => "类别") %>
<%= f.input :is_hidden %>
<%= f.submit "Submit", disable_with: "Submiting...", class: "btn btn-default" %>
<% end %>